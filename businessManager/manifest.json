{
    "manifest_version":2,
    "name":"Number Cruncher",
    "version":"1.1",
    "description":"A Budget Controller extension.",
    "icons":{
        "128":"icon128.png",
        "48":"icon48.png",
        "16":"icon16.png"
    },
    "browser_action":{
        "default_icon":"icon16.png",
        "default_popup":"Curnpopup.html"
    },
    "options_page": "options.html",
    "background":{
        "scripts":["eventPage.js"],
        "persistent":false
    },
    "permissions":[
        "storage",
        "notifications",
        "contextMenus"
    ]
}

